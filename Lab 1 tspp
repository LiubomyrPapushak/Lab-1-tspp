using System;

public class Program
    {

    private static void Main()
        {


            int lenght = 4;
        double vollume = Math.Pow(lenght, 3);
        double surfaceArea = 6 * Math.Pow(lenght, 2);

        Console.WriteLine
            ("О'бєм куба, м3: " + vollume);
        Console.WriteLine
            ("Площа поверхні куба, м2: " + surfaceArea);

        }
            
        }
